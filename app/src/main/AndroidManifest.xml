<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hoangnd.noteyuumi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/notebook"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NoteYuumi">
        <activity
            android:name=".note.NoteActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addnote.AddNoteActivity" />
        <activity android:name=".notedetail.NoteDetailActivity"></activity>
    </application>

</manifest>